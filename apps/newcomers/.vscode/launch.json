  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "~/.nvm/versions/node/v10.15.1/bin/npm",
      "runtimeArgs": [
        "start"
      ],
      "port": 9229
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Newcomers Form",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "preLaunchTask": "webpack: dev server"
    }
  ]
}